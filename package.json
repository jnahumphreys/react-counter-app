  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "plugin:storybook/recommended"
    ],
    "plugins": [
      "@emotion"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint",
    "*": [
      "prettier --ignore-unknown --write"
