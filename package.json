{
  "name": "react-counter-app",
  "version": "1.1.0",
  "description": "React counter app",
  "keywords": [],
  "homepage": "https://binaryjim.github.io/react-counter-app",
  "license": "MIT",
  "author": {
    "name": "James Humphreys",
    "url": "https://github.com/BinaryJim"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BinaryJim/react-tasks-app.git"
  },
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom --setupFiles ./src/setupTests.js",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint-js": "eslint './src/**/*.{js, jsx}'",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-scss": "stylelint './src/**/*.scss'",
    "lint-scss:fix": "npm run lint-scss -- --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public",
      "!<rootDir>/coverage",
      "!<rootDir>/build"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "@emotion"
    ]
  }
}
