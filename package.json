{
  "name": "react-counter-app",
  "version": "1.12.0",
  "description": "React counter app",
  "engines": {
    "node": ">=16.15.1 <17.0.0"
  },
  "keywords": [],
  "homepage": "https://binaryjim.github.io/react-counter-app",
  "license": "MIT",
  "author": {
    "name": "James Humphreys",
    "url": "https://github.com/BinaryJim"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BinaryJim/react-tasks-app.git"
  },
  "bugs": {
    "url": "https://github.com/BinaryJim/react-counter-app/issues"
  },
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@testing-library/cypress": "^8.0.3",
    "cypress": "^10.7.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "start:ci": "BROWSER=none CI=true npm start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "check-formatting": "prettier --check 'src/**/*.{ts,tsx}'",
    "fix-formatting": "npm run check-formatting -- --write",
    "test": "cypress open",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "@emotion"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint",
    "*": [
      "prettier --ignore-unknown --write"
    ]
  }
}
